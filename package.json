{
  "name": "initial-react-full-stack",
  "version": "1.0.0",
  "description": "Updated boilerplate for building a full stack react app.",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --mode production && go build",
    "server": "nodemon --watch server -e \".go\" --exec \"./start-server.sh\"",
    "start": "yarpm run build && ./go-magic-mirror",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "dev": "concurrently \"yarpm run server\" \"yarpm run client\"",
    "install-go": "go get ./..."
  },
  "author": "Erik Malm",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "express": "^4.17.1",
    "global": "^4.4.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-pose": "^4.0.10",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.0.1",
    "css-loader": "^3.3.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-google": "^0.14.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.13.0",
    "nodemon": "^2.0.1",
    "run-if-changed": "^0.0.9",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.1",
    "url-loader": "^3.0.0",
